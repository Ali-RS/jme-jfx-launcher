<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="640.0" styleClass="background" stylesheets="@launcher.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jayfella.launcher.controller.LauncherController">
   <children>
      <VBox layoutX="310.0" layoutY="100.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button fx:id="playButton" mnemonicParsing="false" onAction="#playButtonPressed" styleClass="menu-button" text="Play" />
            <Button mnemonicParsing="false" onAction="#optionsButtonPressed" styleClass="menu-button" text="Options" />
            <Button mnemonicParsing="false" onAction="#closeButtonPressed" styleClass="menu-button" text="Exit" />
         </children>
      </VBox>
      <Label fx:id="titleLabel" layoutX="14.0" layoutY="160.0" styleClass="mainTitle" text="Label" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</AnchorPane>
